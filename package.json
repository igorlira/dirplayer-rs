{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "build/electron.cjs",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/chrome": "^0.0.269",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.3.2",
    "dirplayer-js-api": "file:dirplayer-js-api",
    "electron-is-dev": "^3.0.1",
    "flexlayout-react": "^0.8.17",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-tree": "^0.18.0",
    "react-redux": "^8.1.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "the-new-css-reset": "^1.9.0",
    "typescript": "^4.9.5",
    "usehooks-ts": "^2.9.1",
    "vm-rust": "file:vm-rust/pkg",
    "web-vitals": "^2.1.4"
  },
  "homepage": "./",
  "productName": "DirPlayer",
  "build": {
    "extends": null,
    "appId": "com.dirplayer.studio",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-extension": "vite build -c vite.config.extension.js",
    "build-vm": "cd vm-rust && wasm-pack build --target web",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.25",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^9.2.1",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "vite": "^3.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "wait-on": "^9.0.1"
  }
}
